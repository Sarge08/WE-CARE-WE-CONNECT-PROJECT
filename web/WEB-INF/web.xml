<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Book_meals</servlet-name>
        <servlet-class>Book_meals</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Book_medicine</servlet-name>
        <servlet-class>Book_medicine</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>book_oxygen</servlet-name>
        <servlet-class>book_oxygen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adlogin</servlet-name>
        <servlet-class>adlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgot</servlet-name>
        <servlet-class>forgot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgotp</servlet-name>
        <servlet-class>forgotp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgotpwd</servlet-name>
        <servlet-class>forgotpwd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Book_meals</servlet-name>
        <url-pattern>/Book_meals</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Book_medicine</servlet-name>
        <url-pattern>/Book_medicine</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>book_oxygen</servlet-name>
        <url-pattern>/book_oxygen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adlogin</servlet-name>
        <url-pattern>/adlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgot</servlet-name>
        <url-pattern>/forgot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgotp</servlet-name>
        <url-pattern>/forgotp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgotpwd</servlet-name>
        <url-pattern>/forgotpwd</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
